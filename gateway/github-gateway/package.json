{
  "name": "github-gateway",
  "version": "1.0.0",
  "description": "Webhook handler for GitHub events",
  "author": "Clecio Varjao <1348549+cvarjao@users.noreply.github.com>",
  "license": "ISC",
  "repository": "https://github.com//pipeline-bot.git",
  "homepage": "https://github.com//pipeline-bot",
  "bugs": "https://github.com//pipeline-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prestart": "tsc -p tsconfig.json",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./lib/src/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@types/nock": "^9.3.0",
    "nock": "^10.0.0",
    "probot": "^7.2.0",
    "tf-hcl": "^0.1.0",
    "toposort-class": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^11.9.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.0.0",
    "mock-spawn": "^0.2.6",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.1",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
