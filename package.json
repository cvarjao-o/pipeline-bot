{
  "name": "pipeline-bot",
  "version": "1.0.0",
  "description": "#How to build ## prerequisites - node 8.3.0+ - npm 6+",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node server.js",
    "test": "mocha",
    "pbts": "pbjs -t static-module -w commonjs -o build/compiled.js src/service.proto && pbjs -t static-module -w commonjs -o build/compiled.ts build/compiled.js",
    "protoc": "protoc --js_out=src/generated src/service.proto"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cvarjao-o/pipeline-bot.git"
  },
  "author": "Clecio Varjao",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cvarjao-o/pipeline-bot/issues"
  },
  "homepage": "https://github.com/cvarjao-o/pipeline-bot#readme",
  "devDependencies": {
    "@types/node": "^12.0.8",
    "protobufjs": "~6.8.8",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.1",
    "grpc": "^1.21.1",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0"
  }
}
